{
  "kind": "collectionType",
  "collectionName": "home_banners",
  "info": {
    "singularName": "home-banner",
    "pluralName": "home-banners",
    "displayName": "Home Banner",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text"
    },
    "bannerimg": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "button": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "banner_number": {
      "type": "enumeration",
      "enum": [
        "one",
        "two"
      ]
    },
    "backgroundColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
